          "block": {
            "attributes": {
              "id": {
                "type": "string",
                "computed": true
              },
              "project_id": {
                "type": "string",
                "description": "Specifies the CodeArts project ID.",
                "required": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "tags": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "color": "string",
                      "id": "string",
                      "name": "string"
                    }
                  ]
                ],
                "description": "Indicates the tag list.",
                "computed": true
              }
            }
          }
        }
      }
    }
  }
}
