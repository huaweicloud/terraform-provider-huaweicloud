{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "data_source_schemas": {
        "huaweicloud_identityv5_policies": {
          "block": {
            "attributes": {
              "id": {
                "type": "string",
                "computed": true
              },
              "only_attached": {
                "type": "bool",
