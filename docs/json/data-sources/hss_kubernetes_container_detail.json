{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "data_source_schemas": {
        "huaweicloud_hss_kubernetes_container_detail": {
          "block": {
            "attributes": {
              "container_id": {
                "type": "string",
                "required": true
              },
              "enable_simulate": {
                "type": "string",
                "computed": true
              },
              "enterprise_project_id": {
                "type": "string",
                "optional": true
              },
              "extra": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "linux": [
                        "list",
                        [
                          "object",
                          {
                            "os": "string"
                          }
                        ]
                      ],
                      "mysql": [
                        "list",
                        [
                          "object",
                          {
                            "custom_path": "string"
                          }
                        ]
                      ],
                      "openvpn": [
                        "list",
                        [
                          "object",
                          {
                            "outside_ip": "string",
                            "outside_port": "string"
                          }
                        ]
                      ],
                      "rdp": [
                        "list",
                        [
                          "object",
                          {
                            "proto_env": "string",
                            "system": "string"
                          }
                        ]
                      ]
                    }
                  ]
                ],
                "computed": true
              },
              "hosts": {
                "type": "string",
                "computed": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "service_name": {
                "type": "string",
                "computed": true
              },
              "service_password": {
                "type": "number",
                "computed": true
              },
              "service_port_list": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "desc": "string",
                      "port": "number",
                      "protocol": "string",
                      "type": "string",
                      "user_port": "number"
                    }
                  ]
                ],
                "computed": true
              },
              "service_username": {
                "type": "string",
                "computed": true
              }
            }
          },
          "product": {
            "short": "HSS",
            "name": "Host Security Service",
            "catalog": "Security & Compliance",
            "name_cn": "企业主机安全",
            "catalog_cn": "安全与合规"
          }
        }
      }
    }
  }
}
