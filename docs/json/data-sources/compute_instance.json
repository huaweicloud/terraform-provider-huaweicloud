{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "data_source_schemas": {
        "huaweicloud_compute_instance": {
          "block": {
            "attributes": {
              "availability_zone": {
                "type": "string",
                "computed": true
              },
              "charging_mode": {
                "type": "string",
                "computed": true
              },
              "enterprise_project_id": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "expired_time": {
                "type": "string",
                "computed": true
              },
              "fixed_ip_v4": {
                "type": "string",
                "optional": true
              },
              "flavor_id": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "flavor_name": {
                "type": "string",
                "computed": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "image_id": {
                "type": "string",
                "computed": true
              },
              "image_name": {
                "type": "string",
                "computed": true
              },
              "instance_id": {
                "type": "string",
                "optional": true
              },
              "key_pair": {
                "type": "string",
                "computed": true
              },
              "name": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "network": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "fixed_ip_v4": "string",
                      "fixed_ip_v6": "string",
                      "mac": "string",
                      "port": "string",
                      "uuid": "string"
                    }
                  ]
                ],
                "computed": true
              },
              "public_ip": {
                "type": "string",
                "computed": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "scheduler_hints": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "group": "string"
                    }
                  ]
                ],
                "computed": true
              },
              "security_group_ids": {
                "type": [
                  "list",
                  "string"
                ],
                "computed": true
              },
              "security_groups": {
                "type": [
                  "list",
                  "string"
                ],
                "computed": true
              },
              "status": {
                "type": "string",
                "computed": true
              },
              "system_disk_id": {
                "type": "string",
                "computed": true
              },
              "tags": {
                "type": [
                  "map",
                  "string"
                ],
                "optional": true,
                "computed": true
              },
              "user_data": {
                "type": "string",
                "computed": true
              },
              "volume_attached": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "boot_index": "number",
                      "is_sys_volume": "bool",
                      "pci_address": "string",
                      "size": "number",
                      "type": "string",
                      "volume_id": "string"
                    }
                  ]
                ],
                "computed": true
              }
            }
          },
          "product": {
            "short": "ECS",
            "name": "Elastic Cloud Server",
            "catalog": "Compute",
            "name_cn": "弹性云服务器",
            "catalog_cn": "计算"
          }
        }
      }
    }
  }
}
