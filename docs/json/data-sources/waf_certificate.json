              },
              "region": {
                "type": "string",
                "computed": true
              }
            }
          },
          "product": {
            "short": "WAF",
            "name": "Web Application Firewall",
            "catalog": "Security & Compliance",
            "name_cn": "Web应用防火墙",
            "catalog_cn": "安全与合规"
          }
        }
      }
    }
  }
}
