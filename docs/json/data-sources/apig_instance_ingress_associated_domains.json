              },
              "domains": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "group_id": "string",
                      "group_name": "string",
                      "name": "string"
                    }
                  ]
                ],
                "description": "The list of domain information bound to the ingress port that matched the filter parameters.",
