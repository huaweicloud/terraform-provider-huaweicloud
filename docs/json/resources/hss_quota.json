{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_hss_quota": {
          "block": {
            "attributes": {
              "auto_renew": {
                "type": "string",
                "optional": true
              },
              "charging_mode": {
                "type": "string",
                "computed": true
              },
              "enterprise_project_id": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "enterprise_project_name": {
                "type": "string",
                "computed": true
              },
              "expire_time": {
                "type": "number",
                "computed": true
              },
              "host_id": {
                "type": "string",
                "computed": true
              },
              "host_name": {
                "type": "string",
                "computed": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "is_trial_quota": {
                "type": "bool",
                "computed": true
              },
              "period": {
                "type": "number",
                "required": true,
                "forcenew": true
              },
              "period_unit": {
                "type": "string",
                "required": true,
                "forcenew": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "shared_quota": {
                "type": "string",
                "computed": true
              },
              "status": {
                "type": "string",
                "computed": true
              },
              "tags": {
                "type": [
                  "map",
                  "string"
                ],
                "optional": true,
                "computed": true
              },
              "used_status": {
                "type": "string",
                "computed": true
              },
              "version": {
                "type": "string",
                "required": true,
                "forcenew": true
              }
            }
          },
          "product": {
            "short": "HSS",
            "name": "Host Security Service",
            "catalog": "Security & Compliance",
            "name_cn": "企业主机安全",
            "catalog_cn": "安全与合规"
          }
        }
      }
    }
  }
}
