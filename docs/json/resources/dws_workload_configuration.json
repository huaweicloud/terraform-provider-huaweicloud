{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_dws_workload_configuration": {
          "block": {
            "attributes": {
              "cluster_id": {
                "type": "string",
                "description": "Specifies the DWS cluster ID.",
                "required": true,
                "forcenew": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "max_concurrency_num": {
                "type": "string",
                "description": "Specifies the maximum number of concurrent tasks on a single CN.",
                "optional": true,
                "computed": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "workload_switch": {
                "type": "string",
                "description": "Specifies the workload management switch.",
                "required": true
              }
            }
          },
          "product": {
            "short": "DWS",
            "name": "Data Warehouse Service",
            "catalog": "Analytics",
            "name_cn": "数据仓库服务",
            "catalog_cn": "大数据"
          }
        }
      }
    }
  }
}
