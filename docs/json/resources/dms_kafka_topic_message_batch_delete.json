{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_dms_kafka_topic_message_batch_delete": {
          "block": {
            "attributes": {
              "id": {
                "type": "string",
                "computed": true
              },
              "instance_id": {
                "type": "string",
                "description": "The ID of the Kafka instance.",
                "required": true
              },
              "region": {
                "type": "string",
                "description": "The region where the topic messages to be deleted are located.",
                "computed": true
              },
              "result": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "error_code": "string",
                      "partition": "number",
                      "result": "string"
                    }
                  ]
                ],
                "description": "The result of the message delete operation.",
                "computed": true
              },
              "topic": {
                "type": "string",
                "description": "The name of the topic.",
                "required": true
              }
            },
            "block_types": {
              "partitions": {
                "block": {
                  "attributes": {
                    "offset": {
                      "type": "number",
                      "description": "The offset of the message to be deleted.",
                      "required": true
                    },
                    "partition": {
                      "type": "number",
                      "description": "The number of the partition.",
                      "required": true
                    }
                  },
                  "description": "The partition configuration list to which the messages to be deleted belong."
                },
                "nesting_mode": "list",
                "min_items": 1
              }
            }
          },
          "product": {
            "short": "Kafka",
            "name": "Distributed Message Service for Kafka",
            "catalog": "Middleware",
            "name_cn": "分布式消息服务 Kafka",
            "catalog_cn": "应用中间件"
          }
        }
      }
    }
  }
}
