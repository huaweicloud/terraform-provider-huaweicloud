{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_dns_endpoint": {
          "block": {
            "attributes": {
              "created_at": {
                "type": "string",
                "description": "The creation time of the endpoint.",
                "computed": true
              },
              "direction": {
                "type": "string",
                "description": "The direction of the endpoint.",
                "required": true,
                "forcenew": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "name": {
                "type": "string",
                "description": "The name of the endpoint.",
                "required": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "resolver_rule_count": {
                "type": "number",
                "description": "The number of bound resolver rules.",
                "computed": true
              },
              "status": {
                "type": "string",
                "description": "The status of endpoint.",
                "computed": true
              },
              "updated_at": {
                "type": "string",
                "description": "The latest update time of the endpoint.",
                "computed": true
              },
              "vpc_id": {
