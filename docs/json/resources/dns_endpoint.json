                      "computed": true
                    },
                    "subnet_id": {
                      "type": "string",
                      "description": "The subnet ID of the IP address.",
                      "required": true
                    },
                    "updated_at": {
                      "type": "string",
                      "description": "The latest update time of the endpoint.",
                      "computed": true
                    }
                  },
                  "description": "The list of the IP addresses of the endpoint."
                },
                "nesting_mode": "list",
                "min_items": 2,
                "max_items": 6
              }
            }
          },
          "product": {
            "short": "DNS",
            "name": "Domain Name Service",
            "catalog": "Networking",
            "name_cn": "云解析服务",
            "catalog_cn": "网络"
          }
