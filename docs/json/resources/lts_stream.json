{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_lts_stream": {
          "block": {
            "attributes": {
              "created_at": {
                "type": "string",
                "computed": true
              },
              "enterprise_project_id": {
                "type": "string",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "filter_count": {
                "type": "number",
                "computed": true
              },
              "group_id": {
                "type": "string",
                "required": true,
                "forcenew": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "is_favorite": {
                "type": "bool",
                "description": "Whether to favorite the log stream.",
                "optional": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "stream_name": {
                "type": "string",
                "required": true,
                "forcenew": true
              },
              "tags": {
                "type": [
                  "map",
                  "string"
                ],
                "optional": true,
                "computed": true
              },
              "ttl_in_days": {
                "type": "number",
                "optional": true,
                "computed": true
              }
            }
          },
          "product": {
            "short": "LTS",
            "name": "Log Tank Service",
            "catalog": "Management & Governance",
            "name_cn": "云日志服务",
            "catalog_cn": "管理与监管"
          }
        }
      }
    }
  }
}
