{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_swr_image_permissions": {
          "block": {
            "attributes": {
              "id": {
                "type": "string",
                "computed": true
              },
              "organization": {
                "type": "string",
                "description": "Specifies the name of the organization.",
                "required": true,
                "forcenew": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "repository": {
                "type": "string",
                "description": "Specifies the name of the repository.",
                "required": true,
                "forcenew": true
              },
              "self_permission": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "permission": "string",
                      "user_id": "string",
                      "user_name": "string"
                    }
                  ]
                ],
                "description": "Indicates the permission information of current user.",
                "computed": true
              }
            },
            "block_types": {
              "users": {
                "block": {
                  "attributes": {
                    "permission": {
                      "type": "string",
                      "description": "Specifies the user permission of the existing HuaweiCloud user.",
                      "required": true
                    },
                    "user_id": {
                      "type": "string",
                      "description": "Specifies the ID of the existing HuaweiCloud user.",
                      "required": true
                    },
                    "user_name": {
                      "type": "string",
                      "description": "Specifies the name of the existing HuaweiCloud user.",
                      "optional": true,
                      "computed": true
                    }
                  },
                  "description": "Specifies the users to access to the image (repository)."
                },
                "nesting_mode": "list",
                "min_items": 1
              }
            }
          },
          "product": {
            "short": "SWR",
            "name": "容器镜像服务",
            "catalog": "Containers",
            "name_cn": "容器镜像服务",
            "catalog_cn": "容器"
          }
        }
      }
    }
  }
}
