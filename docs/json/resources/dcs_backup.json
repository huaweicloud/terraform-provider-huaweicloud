                "computed": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "size": {
                "type": "number",
                "description": "Indicates the size of the backup file (byte).",
                "computed": true
              },
              "status": {
                "type": "string",
                "description": "Indicates the backup status.",
                "computed": true
              },
              "type": {
                "type": "string",
                "description": "Indicates the backup type.",
                "computed": true
              }
            }
          },
          "product": {
            "short": "DCS",
            "name": "Distributed Cache Service",
            "catalog": "Middleware",
            "name_cn": "分布式缓存服务",
            "catalog_cn": "应用中间件"
          }
        }
      }
    }
  }
}
