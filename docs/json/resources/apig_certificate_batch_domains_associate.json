{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_apig_certificate_batch_domains_associate": {
          "block": {
            "attributes": {
              "certificate_id": {
                "type": "string",
                "description": "The ID of the SSL certificate.",
                "required": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "instance_id": {
                "type": "string",
                "description": "The ID of the instance to which the certificate and domains belong.",
                "required": true
              },
              "region": {
                "type": "string",
                "description": "The region where the SSL certificate and domains are located.",
                "computed": true
              },
              "verify_disabled_domain_names": {
                "type": [
                  "set",
                  "string"
                ],
                "description": "The domain list to be disabled client certificate verification.",
                "optional": true
              },
              "verify_enabled_domain_names": {
                "type": [
                  "set",
                  "string"
                ],
                "description": "The domain list to be enabled client certificate verification.",
                "optional": true
              }
            }
          },
          "product": {
            "short": "APIG",
            "name": "API Gateway",
            "catalog": "Middleware",
            "name_cn": "API网关",
            "catalog_cn": "应用中间件"
          }
        }
      }
    }
  }
}
