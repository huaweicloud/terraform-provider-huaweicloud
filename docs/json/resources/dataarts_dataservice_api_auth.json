                "forcenew": true
              },
              "region": {
                "type": "string",
                "description": "The region where the API and APP(s) are located."
              },
              "workspace_id": {
                "type": "string",
                "description": "The workspace ID to which the API and APP(s) belong.",
                "required": true,
                "forcenew": true
              }
            }
          },
          "product": {
            "short": "DataArtsStudio",
            "name": "DataArts Studio",
            "catalog": "Analytics",
            "name_cn": "数据治理中心",
