{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_dataarts_dataservice_api_auth": {
          "block": {
            "attributes": {
              "api_id": {
                "type": "string",
                "description": "The ID of the published API.",
                "required": true,
                "forcenew": true
              },
              "app_ids": {
                "type": [
                  "list",
                  "string"
                ],
                "description": "The list of application IDs that used to authorize API.",
                "required": true,
                "forcenew": true
              },
              "id": {
                "type": "string",
