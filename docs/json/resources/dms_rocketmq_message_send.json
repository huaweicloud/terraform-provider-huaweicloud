{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_dms_rocketmq_message_send": {
          "block": {
            "attributes": {
              "body": {
                "type": "string",
                "description": "The content of the message to be sent.",
                "required": true
              },
              "broker_name": {
                "type": "string",
                "description": "The broker name of the message.",
                "computed": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "instance_id": {
                "type": "string",
                "description": "The ID of the RocketMQ instance.",
                "required": true
              },
              "msg_id": {
                "type": "string",
                "description": "The ID of the message that was sent.",
                "computed": true
              },
              "queue_id": {
                "type": "number",
                "description": "The queue ID of the message.",
                "computed": true
              },
              "queue_offset": {
                "type": "number",
                "description": "The queue offset of the message.",
                "computed": true
              },
              "region": {
                "type": "string",
