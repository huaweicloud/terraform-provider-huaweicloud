              }
            },
            "block_types": {
              "property_list": {
                "block": {
                  "attributes": {
                    "name": {
                      "type": "string",
                      "description": "The name of the property.",
                      "required": true
                    },
                    "value": {
                      "type": "string",
                      "description": "The value of the property.",
                      "required": true
                    }
                  },
                  "description": "The list of message properties."
                },
                "nesting_mode": "list"
              }
            }
          },
          "product": {
            "short": "RocketMQ",
            "name": "Distributed Message Service for RocketMQ",
