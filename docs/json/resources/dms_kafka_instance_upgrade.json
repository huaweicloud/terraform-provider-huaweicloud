{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_dms_kafka_instance_upgrade": {
          "block": {
            "attributes": {
              "execute_at": {
                "type": "number",
                "description": "The scheduled time in Unix timestamp format, in milliseconds.",
                "optional": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "instance_id": {
                "type": "string",
                "description": "The ID of the Kafka instance.",
                "required": true
              },
              "is_schedule": {
                "type": "bool",
                "description": "Whether to execute as a scheduled task.",
                "optional": true
              },
              "region": {
                "type": "string",
                "description": "The region where the Kafka instance to be upgraded is located.",
                "computed": true
              }
            }
          },
          "product": {
            "short": "Kafka",
            "name": "Distributed Message Service for Kafka",
            "catalog": "Middleware",
            "name_cn": "分布式消息服务 Kafka",
            "catalog_cn": "应用中间件"
          }
        }
      }
    }
  }
}
