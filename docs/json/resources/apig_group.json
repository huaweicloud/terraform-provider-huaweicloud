{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_apig_group": {
          "block": {
            "attributes": {
              "description": {
                "type": "string",
                "description": "The group description.",
                "optional": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "instance_id": {
                "type": "string",
                "description": "The ID of the dedicated instance to which the group belongs.",
                "required": true,
                "forcenew": true
              },
              "name": {
                "type": "string",
                "description": "The group name.",
                "required": true
              },
              "region": {
                "type": "string",
                "description": "The region where the dedicated instance is located.",
                "computed": true
              },
              "registration_time": {
                "type": "string",
                "description": "The registration time.",
                "computed": true
              },
              "updated_at": {
                "type": "string",
                "description": "The latest update time of the group.",
                "computed": true
              }
            },
            "block_types": {
              "environment": {
                "block": {
                  "attributes": {
                    "environment_id": {
                      "type": "string",
                      "description": "The ID of the environment to which the variables belongs.",
                      "required": true
                    }
                  },
                  "block_types": {
                    "variable": {
                      "block": {
                        "attributes": {
                          "id": {
                            "type": "string",
                            "description": "The ID of the variable that the group has.",
                            "computed": true
                          },
                          "name": {
                            "type": "string",
                            "description": "The variable name.",
                            "required": true
                          },
                          "value": {
                            "type": "string",
                            "description": "The variable value.",
                            "required": true
                          }
                        },
                        "description": "The array of one or more environment variables."
                      },
                      "nesting_mode": "set",
                      "min_items": 1
                    }
                  },
                  "description": "The array of one or more environments of the associated group."
                },
                "nesting_mode": "set"
              }
            }
          },
          "product": {
            "short": "APIG",
            "name": "API网关",
            "catalog": "Middleware",
            "name_cn": "API网关",
            "catalog_cn": "应用中间件"
          }
        }
      }
    }
  }
}
