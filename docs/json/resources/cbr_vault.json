{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_cbr_vault": {
          "block": {
            "attributes": {
              "allocated": {
                "type": "number",
                "description": "The allocated capacity, in GB.",
                "computed": true
              },
              "auto_bind": {
                "type": "bool",
                "description": "Whether automatic association is supported.",
                "optional": true,
                "computed": true
              },
              "auto_expand": {
                "type": "bool",
                "description": "Whether to enable auto capacity expansion for the vault.",
                "optional": true,
                "computed": true
              },
              "auto_renew": {
                "type": "string",
                "optional": true
              },
              "backup_name_prefix": {
                "type": "string",
                "description": "The backup name prefix.",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "bind_rules": {
                "type": [
                  "map",
                  "string"
                ],
                "description": "The rules for automatic association.",
                "optional": true
              },
              "charging_mode": {
                "type": "string",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "cloud_type": {
                "type": "string",
                "description": "The cloud type of the vault.",
                "required": true,
                "forcenew": true
              },
              "consistent_level": {
                "type": "string",
                "description": "The consistent level (specification) of the vault.",
                "optional": true,
                "default": "crash_consistent"
              },
              "enterprise_project_id": {
                "type": "string",
                "description": "The enterprise project ID to which the vault belongs.",
                "optional": true,
                "computed": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "is_multi_az": {
                "type": "bool",
                "description": "Whether multiple availability zones are used for backing up.",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "locked": {
                "type": "bool",
                "description": "Locked status of the vault.",
                "optional": true,
                "computed": true
              },
              "name": {
                "type": "string",
                "description": "The name of the vault.",
                "required": true
              },
              "period": {
                "type": "number",
                "optional": true,
                "forcenew": true
              },
              "period_unit": {
                "type": "string",
                "optional": true,
                "forcenew": true
              },
              "protection_type": {
                "type": "string",
                "description": "The protection type of the vault.",
                "required": true,
                "forcenew": true
              },
              "region": {
                "type": "string",
                "description": "The region where the vault is located.",
                "computed": true
              },
              "size": {
                "type": "number",
                "description": "The capacity of the vault, in GB.",
                "required": true
              },
              "spec_code": {
                "type": "string",
                "description": "The specification code.",
                "computed": true
              },
              "status": {
                "type": "string",
                "description": "The vault status.",
                "computed": true
              },
              "storage": {
                "type": "string",
                "description": "The name of the bucket for the vault.",
                "computed": true
              },
              "tags": {
                "type": [
                  "map",
                  "string"
                ],
                "optional": true
              },
              "type": {
                "type": "string",
                "description": "The type of the vault.",
                "required": true,
                "forcenew": true
              },
              "used": {
                "type": "number",
                "description": "The used capacity, in GB.",
                "computed": true
              }
            },
            "block_types": {
              "policy": {
                "block": {
                  "attributes": {
                    "destination_vault_id": {
                      "type": "string",
                      "description": "The ID of destination vault to which the replication policy will associated.",
                      "optional": true
                    },
                    "id": {
                      "type": "string",
                      "description": "The policy ID.",
                      "required": true
                    }
                  },
                  "description": "The policy details to associate with the CBR vault."
                },
                "nesting_mode": "set"
              },
              "resources": {
                "block": {
                  "attributes": {
                    "excludes": {
                      "type": [
                        "set",
                        "string"
                      ],
                      "description": "The array of disk IDs which will be excluded in the backup.",
                      "optional": true,
                      "computed": true
                    },
                    "includes": {
                      "type": [
                        "set",
                        "string"
                      ],
                      "description": "The array of disk or SFS file systems which will be included in the backup.",
                      "optional": true,
                      "computed": true
                    },
                    "server_id": {
                      "type": "string",
                      "description": "The ID of the ECS instance to be backed up.",
                      "optional": true
                    }
                  },
                  "description": "The array of one or more resources to attach to the CBR vault."
                },
                "nesting_mode": "set"
              }
            }
          },
          "product": {
            "short": "CBR",
            "name": "Cloud Backup and Recovery",
            "catalog": "Storage",
            "name_cn": "云备份",
            "catalog_cn": "存储"
          }
        }
      }
    }
  }
}
