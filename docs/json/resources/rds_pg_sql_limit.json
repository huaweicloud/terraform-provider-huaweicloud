{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_rds_pg_sql_limit": {
          "block": {
            "attributes": {
              "db_name": {
                "type": "string",
                "description": "Specifies the name of the database.",
                "required": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "instance_id": {
                "type": "string",
                "description": "Specifies the ID of the RDS PostgreSQL instance.",
                "required": true
              },
              "is_effective": {
                "type": "bool",
                "description": "Indicates whether the SQL limit is effective.",
                "computed": true
              },
              "max_concurrency": {
                "type": "number",
                "description": "Specifies the number of SQL statements executed simultaneously",
                "required": true
              },
              "max_waiting": {
                "type": "number",
                "description": "Specifies the max waiting time in seconds.",
                "required": true
              },
              "query_id": {
                "type": "string",
                "description": "Specifies the query ID",
                "optional": true
              },
              "query_string": {
                "type": "string",
                "description": "Specifies the text form of SQL statement.",
                "optional": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "search_path": {
                "type": "string",
                "description": "Specifies the query order for names that are not schema qualified.",
                "optional": true
              },
              "sql_limit_id": {
                "type": "string",
                "description": "Indicates the ID of SQL limit.",
                "computed": true
              },
              "switch": {
                "type": "string",
                "description": "Specifies the SQL limit switch.",
                "optional": true,
                "computed": true
              }
            }
          },
          "product": {
            "short": "RDS",
            "name": "Relational Database Service",
            "catalog": "Databases",
            "name_cn": "云数据库",
            "catalog_cn": "数据库"
          }
        }
      }
    }
  }
}
