{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_identitycenter_password_policy": {
          "block": {
            "attributes": {
              "id": {
                "type": "string",
                "computed": true
              },
              "identity_store_id": {
                "type": "string",
                "required": true
              },
              "max_password_age": {
                "type": "number",
                "optional": true
              },
              "minimum_password_length": {
                "type": "number",
                "optional": true
              },
              "password_reuse_prevention": {
                "type": "number",
                "optional": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "require_lowercase_characters": {
                "type": "bool",
                "optional": true
              },
              "require_numbers": {
                "type": "bool",
                "optional": true
              },
              "require_symbols": {
                "type": "bool",
                "optional": true
              },
              "require_uppercase_characters": {
                "type": "bool",
                "optional": true
              }
            }
          }
        }
      }
    }
  }
}
