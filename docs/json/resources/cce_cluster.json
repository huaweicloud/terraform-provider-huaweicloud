{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_cce_cluster": {
          "block": {
            "attributes": {
              "alias": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "authenticating_proxy_ca": {
                "type": "string",
                "optional": true,
                "forcenew": true
              },
              "authenticating_proxy_cert": {
                "type": "string",
                "optional": true,
                "forcenew": true
              },
              "authenticating_proxy_private_key": {
                "type": "string",
                "optional": true,
                "forcenew": true
              },
              "authentication_mode": {
                "type": "string",
                "optional": true,
                "forcenew": true,
                "default": "rbac"
              },
              "auto_renew": {
                "type": "string",
                "optional": true
              },
              "category": {
                "type": "string",
                "computed": true
              },
              "certificate_clusters": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "certificate_authority_data": "string",
                      "name": "string",
                      "server": "string"
                    }
                  ]
                ],
                "computed": true
              },
              "certificate_users": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "client_certificate_data": "string",
                      "client_key_data": "string",
                      "name": "string"
                    }
                  ]
                ],
                "computed": true
              },
              "charging_mode": {
                "type": "string",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "cluster_type": {
                "type": "string",
                "optional": true,
                "forcenew": true,
                "default": "VirtualMachine"
              },
              "cluster_version": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "container_network_cidr": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "container_network_type": {
                "type": "string",
                "required": true,
                "forcenew": true
              },
              "custom_san": {
                "type": [
                  "list",
                  "string"
                ],
                "optional": true,
                "computed": true
              },
              "delete_all": {
                "type": "string",
                "optional": true
              },
              "delete_efs": {
                "type": "string",
                "optional": true
              },
              "delete_evs": {
                "type": "string",
                "optional": true
              },
              "delete_obs": {
                "type": "string",
                "optional": true
              },
              "delete_sfs": {
                "type": "string",
                "optional": true
              },
              "description": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "eip": {
                "type": "string",
                "optional": true
              },
              "enable_distribute_management": {
                "type": "bool",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "eni_subnet_cidr": {
                "type": "string",
                "computed": true
              },
              "eni_subnet_id": {
                "type": "string",
                "description": "the IPv4 subnet ID of the subnet where the ENI resides",
                "optional": true,
                "computed": true
              },
              "enterprise_project_id": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "flavor_id": {
                "type": "string",
                "required": true
              },
              "hibernate": {
                "type": "bool",
                "optional": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "ipv6_enable": {
                "type": "bool",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "kube_config_raw": {
                "type": "string",
                "computed": true
              },
              "kube_proxy_mode": {
                "type": "string",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "lts_reclaim_policy": {
                "type": "string",
                "optional": true
              },
              "multi_az": {
                "type": "bool",
                "optional": true,
                "forcenew": true
              },
              "name": {
                "type": "string",
                "required": true,
                "forcenew": true
              },
              "period": {
                "type": "number",
                "optional": true,
                "forcenew": true
              },
              "period_unit": {
                "type": "string",
                "optional": true,
                "forcenew": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "security_group_id": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "service_network_cidr": {
                "type": "string",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "status": {
                "type": "string",
                "computed": true
              },
              "subnet_id": {
                "type": "string",
                "required": true,
                "forcenew": true
              },
              "support_istio": {
                "type": "bool",
                "computed": true
              },
              "tags": {
                "type": [
                  "map",
                  "string"
                ],
                "optional": true
              },
              "timezone": {
                "type": "string",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "vpc_id": {
                "type": "string",
                "required": true,
                "forcenew": true
              }
            },
            "block_types": {
              "component_configurations": {
                "block": {
                  "attributes": {
                    "configurations": {
                      "type": "string",
                      "optional": true
                    },
                    "name": {
                      "type": "string",
                      "required": true
                    }
                  }
                },
                "nesting_mode": "list"
              },
              "encryption_config": {
                "block": {
                  "attributes": {
                    "kms_key_id": {
                      "type": "string",
                      "optional": true,
                      "computed": true,
                      "forcenew": true
                    },
                    "mode": {
                      "type": "string",
                      "optional": true,
                      "computed": true,
                      "forcenew": true
                    }
                  }
                },
                "nesting_mode": "list",
                "forcenew": true,
                "max_items": 1
              },
              "extend_params": {
                "block": {
                  "attributes": {
                    "cluster_az": {
                      "type": "string",
                      "optional": true,
                      "forcenew": true
                    },
                    "cpu_manager_policy": {
                      "type": "string",
                      "optional": true,
                      "forcenew": true
                    },
                    "dec_master_flavor": {
                      "type": "string",
                      "optional": true,
                      "forcenew": true
                    },
                    "docker_umask_mode": {
                      "type": "string",
                      "optional": true,
                      "forcenew": true
                    },
                    "dss_master_volumes": {
                      "type": "string",
                      "optional": true,
                      "forcenew": true
                    },
                    "fix_pool_mask": {
                      "type": "string",
                      "optional": true,
                      "forcenew": true
                    }
                  }
                },
                "nesting_mode": "list",
                "forcenew": true
              },
              "masters": {
                "block": {
                  "attributes": {
                    "availability_zone": {
                      "type": "string",
                      "optional": true,
                      "computed": true,
                      "forcenew": true
                    }
                  }
                },
                "nesting_mode": "list",
                "forcenew": true,
                "max_items": 3
              }
            }
          },
          "product": {
            "short": "CCE",
            "name": "Cloud Container Engine",
            "catalog": "Containers",
            "name_cn": "云容器引擎",
            "catalog_cn": "容器"
          }
        }
      }
    }
  }
}
