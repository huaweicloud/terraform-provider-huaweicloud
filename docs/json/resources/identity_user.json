{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_identity_user": {
          "block": {
            "attributes": {
              "access_type": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "country_code": {
                "type": "string",
                "optional": true
              },
              "create_time": {
                "type": "string",
                "computed": true
              },
              "description": {
                "type": "string",
                "optional": true
              },
              "email": {
                "type": "string",
                "optional": true
              },
              "enabled": {
                "type": "bool",
                "optional": true,
                "default": true
              },
              "external_identity_id": {
                "type": "string",
                "optional": true
              },
              "external_identity_type": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "last_login": {
                "type": "string",
                "computed": true
              },
              "login_protect_verification_method": {
                "type": "string",
                "optional": true
              },
              "name": {
                "type": "string",
                "required": true
              },
              "password": {
                "type": "string",
                "optional": true,
                "sensitive": true
              },
              "password_strength": {
                "type": "string",
                "computed": true
              },
              "phone": {
                "type": "string",
                "optional": true
              },
              "pwd_reset": {
                "type": "bool",
                "optional": true,
                "default": true
              }
            }
          },
          "product": {
            "short": "IAM",
            "name": "Identity and Access Management",
            "catalog": "Management & Governance",
            "name_cn": "统一身份认证服务",
            "catalog_cn": "管理与监管"
          }
        }
      }
    }
  }
}
