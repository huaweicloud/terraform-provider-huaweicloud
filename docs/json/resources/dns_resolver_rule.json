{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_dns_resolver_rule": {
          "block": {
            "attributes": {
              "created_at": {
                "type": "string",
                "description": "The creation time of the resolver rule.",
                "computed": true
              },
              "domain_name": {
                "type": "string",
                "description": "The domain name.",
                "required": true,
                "forcenew": true
              },
              "endpoint_id": {
                "type": "string",
                "description": "The ID of the DNS endpoint to which the resolver rule belongs.",
                "required": true,
                "forcenew": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "name": {
                "type": "string",
                "description": "The resolver rule name.",
                "required": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "rule_type": {
                "type": "string",
                "description": "The rule type of the resolver rule.",
                "computed": true
              },
              "status": {
                "type": "string",
                "description": "The status of the resolver rule.",
                "computed": true
              },
              "updated_at": {
                "type": "string",
                "description": "The latest update time of the resolver rule.",
                "computed": true
              },
              "vpcs": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "status": "string",
                      "vpc_id": "string",
                      "vpc_region": "string"
                    }
                  ]
                ],
                "computed": true
              }
            },
            "block_types": {
              "ip_addresses": {
                "block": {
                  "attributes": {
                    "ip": {
                      "type": "string",
                      "description": "The IP of the IP address.",
                      "optional": true
                    }
                  },
                  "description": "The IP address list of the DNS resolver rule."
                },
                "nesting_mode": "set",
                "min_items": 1
              }
            }
          },
          "product": {
            "short": "DNS",
            "name": "Domain Name Service",
            "catalog": "Networking",
            "name_cn": "云解析服务",
            "catalog_cn": "网络"
          }
        }
      }
    }
  }
}
