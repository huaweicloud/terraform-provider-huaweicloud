{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_apig_plugin_associate": {
          "block": {
            "attributes": {
              "api_ids": {
                "type": [
                  "set",
                  "string"
                ],
                "description": "The list of API IDs to be bound by the plugin.",
                "required": true
              },
              "env_id": {
                "type": "string",
                "description": "The environment ID where the API was published.",
                "required": true,
                "forcenew": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "instance_id": {
                "type": "string",
                "description": "The ID of the dedicated instance to which the plugin belongs.",
                "required": true,
                "forcenew": true
              },
              "plugin_id": {
                "type": "string",
                "description": "The plugin ID.",
                "required": true,
                "forcenew": true
              },
              "region": {
                "type": "string",
                "description": "The region where the plugin is located.",
                "computed": true
              }
            }
          },
          "product": {
            "short": "APIG",
            "name": "API Gateway",
            "catalog": "Middleware",
            "name_cn": "API网关",
            "catalog_cn": "应用中间件"
          }
        }
      }
    }
  }
}
