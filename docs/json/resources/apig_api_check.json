{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_apig_api_check": {
          "block": {
            "attributes": {
              "api_id": {
                "type": "string",
                "description": "The ID of the API to be excluded from the check.",
                "optional": true
              },
              "group_id": {
                "type": "string",
                "description": "The ID of the group to which the API belongs.",
                "optional": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "instance_id": {
                "type": "string",
                "description": "The ID of the dedicated instance to which the API belongs.",
                "required": true
              },
              "match_mode": {
                "type": "string",
                "description": "The matching mode of the API.",
                "optional": true
              },
              "name": {
                "type": "string",
                "description": "The name of the API.",
                "optional": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "req_method": {
                "type": "string",
                "description": "The request method of the API.",
                "optional": true
              },
              "req_uri": {
                "type": "string",
                "description": "The request path of the API.",
                "optional": true
              },
              "type": {
                "type": "string",
                "description": "The type of the API to be checked.",
                "required": true
              }
            }
          },
          "product": {
            "short": "APIG",
            "name": "API Gateway",
            "catalog": "Middleware",
            "name_cn": "API网关",
            "catalog_cn": "应用中间件"
          }
        }
      }
    }
  }
}
