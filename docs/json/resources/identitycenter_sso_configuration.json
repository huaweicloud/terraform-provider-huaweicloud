              },
              "instance_id": {
                "type": "string",
                "required": true
              },
              "max_authentication_age": {
                "type": "string",
                "required": true
              },
              "mfa_mode": {
                "type": "string",
                "required": true
              },
              "no_mfa_signin_behavior": {
                "type": "string",
                "required": true
              },
              "no_password_signin_behavior": {
                "type": "string",
                "required": true
              }
            }
          }
        }
      }
    }
  }
}
