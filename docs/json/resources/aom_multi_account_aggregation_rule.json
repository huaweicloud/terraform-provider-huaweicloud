{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_aom_multi_account_aggregation_rule": {
          "block": {
            "attributes": {
              "id": {
                "type": "string",
                "computed": true
              },
              "instance_id": {
                "type": "string",
                "required": true,
                "forcenew": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "send_to_source_account": {
                "type": "bool",
                "optional": true,
                "computed": true
              }
            },
            "block_types": {
              "accounts": {
                "block": {
                  "attributes": {
                    "id": {
                      "type": "string",
                      "required": true
                    },
                    "join_method": {
                      "type": "string",
                      "optional": true
                    },
                    "joined_at": {
                      "type": "string",
                      "optional": true
                    },
                    "name": {
                      "type": "string",
                      "required": true
                    },
                    "urn": {
                      "type": "string",
                      "optional": true
                    }
                  }
                },
                "nesting_mode": "set",
                "min_items": 1
              },
              "services": {
                "block": {
                  "attributes": {
                    "metrics": {
                      "type": [
                        "set",
                        "string"
                      ],
                      "required": true
                    },
                    "service": {
                      "type": "string",
                      "required": true
                    }
                  }
                },
                "nesting_mode": "set"
              }
            }
          },
          "product": {
            "short": "AOM",
            "name": "Application Operations Management",
            "catalog": "Management & Governance",
            "name_cn": "应用运维管理",
            "catalog_cn": "管理与监管"
          }
        }
      }
    }
  }
}
