{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_identity_token_with_id_token": {
          "block": {
            "attributes": {
              "domain_name": {
                "type": "string",
                "optional": true,
                "forcenew": true
              },
              "expires_at": {
                "type": "string",
                "computed": true
              },
              "groups": {
                "type": [
                  "list",
                  "string"
                ],
                "computed": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "id_token": {
                "type": "string",
                "description": "ID Token of the OpenID Connect Identity Provider",
                "required": true,
                "forcenew": true
              },
              "idp_id": {
                "type": "string",
                "description": "Identity Provider Id",
                "required": true,
                "forcenew": true
              },
              "project_name": {
                "type": "string",
                "optional": true,
                "forcenew": true
              },
              "token": {
                "type": "string",
                "computed": true
              },
              "username": {
                "type": "string",
                "computed": true
              }
            }
          },
          "product": {
            "short": "IAM",
            "name": "Identity and Access Management",
            "catalog": "Management & Governance",
            "name_cn": "统一身份认证服务",
            "catalog_cn": "管理与监管"
          }
        }
      }
    }
  }
}
