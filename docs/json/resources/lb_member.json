{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_lb_member": {
          "block": {
            "attributes": {
              "address": {
                "type": "string",
                "required": true,
                "forcenew": true
              },
              "backend_server_status": {
                "type": "bool",
                "computed": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "name": {
                "type": "string",
                "optional": true
              },
              "operating_status": {
                "type": "string",
                "computed": true
              },
              "pool_id": {
                "type": "string",
                "required": true,
                "forcenew": true
              },
              "protocol_port": {
                "type": "number",
                "required": true,
                "forcenew": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "subnet_id": {
                "type": "string",
                "description": "the IPv4 subnet ID of the subnet in which to access the member",
                "required": true,
                "forcenew": true
              },
              "weight": {
                "type": "number",
                "optional": true,
                "computed": true
              }
            }
          },
          "product": {
            "short": "ELB",
            "name": "Elastic Load Balance",
            "catalog": "Networking",
            "name_cn": "弹性负载均衡",
            "catalog_cn": "网络"
          }
        }
      }
    }
  }
}
