              },
              "region": {
                "type": "string",
                "computed": true
              },
              "server_id": {
                "type": "string",
                "description": "Specifies the CPH server ID.",
                "required": true
              }
            }
          },
          "product": {
            "short": "CPH",
            "name": "Cloud Phone",
