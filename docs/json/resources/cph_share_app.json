            "attributes": {
              "bucket_name": {
                "type": "string",
                "description": "Specifies the OBS bucket name.",
                "required": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "object_path": {
                "type": "string",
                "description": "Specifies the OBS object path.",
                "required": true
              },
              "package_name": {
                "type": "string",
