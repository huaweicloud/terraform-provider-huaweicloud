{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_dli_flinkjar_job": {
          "block": {
            "attributes": {
              "checkpoint_enabled": {
                "type": "bool",
                "description": "Whether to enable the automatic job snapshot function.",
                "optional": true
              },
              "checkpoint_interval": {
                "type": "number",
                "description": "The interval of the snapshot, in seconds.",
                "optional": true,
                "computed": true
              },
              "checkpoint_mode": {
                "type": "number",
                "description": "The mode of the snapshot.",
                "optional": true,
                "computed": true
              },
              "checkpoint_path": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "cu_num": {
                "type": "number",
                "optional": true,
                "default": 2
              },
              "dependency_files": {
                "type": [
                  "list",
                  "string"
                ],
                "optional": true
              },
              "dependency_jars": {
                "type": [
                  "list",
                  "string"
                ],
                "optional": true
              },
              "description": {
                "type": "string",
                "optional": true
              },
              "entrypoint": {
                "type": "string",
                "optional": true
              },
              "entrypoint_args": {
                "type": "string",
                "optional": true
              },
              "execution_agency_urn": {
                "type": "string",
                "description": "The name of the delegation authorized to DLI.",
                "optional": true
              },
              "feature": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "flink_version": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "image": {
                "type": "string",
                "optional": true
              },
              "log_enabled": {
                "type": "bool",
                "optional": true,
                "default": false
              },
              "main_class": {
                "type": "string",
                "optional": true
              },
              "manager_cu_num": {
                "type": "number",
                "optional": true,
                "computed": true
              },
              "name": {
                "type": "string",
                "required": true
              },
              "obs_bucket": {
                "type": "string",
                "optional": true
              },
              "parallel_num": {
                "type": "number",
                "optional": true,
                "computed": true
              },
              "queue_name": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "resource_config_version": {
                "type": "string",
                "description": "The version of the resource configuration.",
                "optional": true,
                "computed": true
              },
              "restart_when_exception": {
                "type": "bool",
                "optional": true,
                "default": false
              },
              "resume_checkpoint": {
                "type": "bool",
                "optional": true
              },
              "resume_max_num": {
                "type": "number",
                "optional": true,
                "default": -1
              },
              "runtime_config": {
                "type": [
                  "map",
                  "string"
                ],
                "optional": true,
                "computed": true
              },
              "smn_topic": {
                "type": "string",
                "optional": true
              },
              "status": {
                "type": "string",
                "computed": true
              },
              "tags": {
                "type": [
                  "map",
                  "string"
                ],
                "optional": true,
                "computed": true
              },
              "tm_cu_num": {
                "type": "number",
                "optional": true,
                "computed": true
              },
              "tm_slot_num": {
                "type": "number",
                "optional": true,
                "computed": true
              }
            },
            "block_types": {
              "resource_config": {
                "block": {
                  "attributes": {
                    "max_slot": {
                      "type": "number",
                      "description": "The number of slots in the TaskManager.",
                      "optional": true,
                      "computed": true
                    },
                    "parallel_number": {
                      "type": "number",
                      "description": "The parallel number of the job.",
                      "optional": true,
                      "computed": true
                    }
                  },
                  "block_types": {
                    "job_manager_resource_spec": {
                      "block": {
                        "attributes": {
                          "cpu": {
                            "type": "number",
                            "description": "The cores of the CPU.",
                            "optional": true,
                            "computed": true
                          },
                          "memory": {
                            "type": "string",
                            "description": "The memory size, in MB or GB (default).",
                            "optional": true,
                            "computed": true
                          }
                        },
                        "description": "The resource specification of the JobManager."
                      },
                      "nesting_mode": "list",
                      "max_items": 1
                    },
                    "task_manager_resource_spec": {
                      "block": {
                        "attributes": {
                          "cpu": {
                            "type": "number",
                            "description": "The cores of the CPU.",
                            "optional": true,
                            "computed": true
                          },
                          "memory": {
                            "type": "string",
                            "description": "The memory size, in MB or GB (default).",
                            "optional": true,
                            "computed": true
                          }
                        },
                        "description": "The resource specification of the TaskManager."
                      },
                      "nesting_mode": "list",
                      "max_items": 1
                    }
                  }
                },
                "nesting_mode": "list",
                "max_items": 1
              }
            }
          },
          "product": {
            "short": "DLI",
            "name": "Data Lake Insight",
            "catalog": "Analytics",
            "name_cn": "数据湖探索",
            "catalog_cn": "大数据"
          }
        }
      }
    }
  }
}
