                      "required": true
                    },
                    "account_type": {
                      "type": "string",
                      "description": "The account type. Valid values are \"SIMPLE\" and \"USER_GROUP\".",
                      "required": true
                    },
                    "domain": {
                      "type": "string",
                      "description": "The domain name. Required for user groups.",
