{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_workspace_application_batch_authorize": {
          "block": {
            "attributes": {
              "app_ids": {
                "type": [
                  "list",
                  "string"
                ],
