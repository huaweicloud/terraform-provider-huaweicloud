                  "attributes": {
                    "account": {
                      "type": "string",
                      "description": "The account name.",
                      "required": true
                    },
                    "account_type": {
                      "type": "string",
                      "description": "The account type. Valid values are \"SIMPLE\" and \"USER_GROUP\".",
