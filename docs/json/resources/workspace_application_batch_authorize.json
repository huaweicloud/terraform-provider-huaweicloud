                    },
                    "domain": {
                      "type": "string",
                      "description": "The domain name. Required for user groups.",
                      "optional": true
                    },
                    "platform_type": {
                      "type": "string",
                      "description": "The platform type. Valid values are \"AD\" and \"LOCAL\".",
                      "optional": true
                    }
                  },
                  "description": "The list of users to be authorized."
                },
                "nesting_mode": "list",
                "max_items": 100
              }
            }
          },
          "product": {
            "short": "Workspace",
            "name": "Workspace",
            "catalog": "Business Applications",
            "name_cn": "云桌面",
