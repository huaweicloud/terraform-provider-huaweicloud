{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_workspace_user": {
          "block": {
            "attributes": {
              "account_expires": {
                "type": "string",
                "optional": true,
                "default": "0"
              },
              "active_type": {
                "type": "string",
                "description": "The activation mode of the user.",
                "optional": true,
                "computed": true
              },
              "description": {
                "type": "string",
                "optional": true
              },
              "disabled": {
                "type": "bool",
                "optional": true
              },
              "email": {
                "type": "string",
                "optional": true
              },
              "enable_change_password": {
                "type": "bool",
                "optional": true,
                "default": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "locked": {
                "type": "bool",
                "computed": true
              },
              "name": {
                "type": "string",
                "required": true,
                "forcenew": true
              },
              "next_login_change_password": {
                "type": "bool",
                "optional": true,
                "default": true
              },
              "password": {
                "type": "string",
                "description": "The initial passowrd of user.",
                "optional": true,
                "sensitive": true
              },
              "password_never_expires": {
                "type": "bool",
                "optional": true
              },
              "phone": {
                "type": "string",
                "description": "The phone number of the user.",
                "optional": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "total_desktops": {
                "type": "number",
                "computed": true
              }
            }
          },
          "product": {
            "short": "Workspace",
            "name": "Workspace",
            "catalog": "Business Applications",
            "name_cn": "云桌面",
            "catalog_cn": "企业应用"
          }
        }
      }
    }
  }
}
