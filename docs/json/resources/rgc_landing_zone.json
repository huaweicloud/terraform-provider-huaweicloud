                "optional": true
              },
              "cloud_trail_type": {
                "type": "bool",
                "optional": true,
                "default": false
              },
              "deny_ungoverned_regions": {
                "type": "bool",
                "optional": true,
                "default": false
              },
              "deployed_version": {
                "type": "string",
                "computed": true
              },
              "home_region": {
                "type": "string",
                "required": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "identity_center_status": {
                "type": "string",
                "optional": true,
                "default": "ENABLE"
