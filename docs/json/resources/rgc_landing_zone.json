                  }
                },
                "nesting_mode": "list",
                "min_items": 1
              },
              "region_configuration_list": {
                "block": {
                  "attributes": {
                    "region": {
                      "type": "string",
                      "required": true
                    },
                    "region_configuration_status": {
                      "type": "string",
                      "required": true
                    }
                  }
                },
                "nesting_mode": "list",
                "min_items": 1
              }
            }
