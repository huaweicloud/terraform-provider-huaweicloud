                "block": {
                  "attributes": {
                    "logging_bucket_name": {
                      "type": "string",
                      "optional": true
                    }
                  },
                  "block_types": {
                    "access_logging_bucket": {
                      "block": {
                        "attributes": {
                          "enable_multi_az": {
                            "type": "bool",
                            "optional": true,
                            "default": false
                          },
                          "retention_days": {
                            "type": "number",
                            "required": true
                          }
                        }
                      },
                      "nesting_mode": "list",
                      "min_items": 1,
                      "max_items": 1
                    },
                    "logging_bucket": {
                      "block": {
                        "attributes": {
                          "enable_multi_az": {
                            "type": "bool",
                            "optional": true,
                            "default": false
                          },
                          "retention_days": {
                            "type": "number",
                            "required": true
                          }
                        }
                      },
                      "nesting_mode": "list",
                      "min_items": 1,
                      "max_items": 1
                    }
                  }
                },
                "nesting_mode": "list",
                "min_items": 1,
                "max_items": 1
              },
              "organization_structure": {
                "block": {
                  "attributes": {
                    "organizational_unit_name": {
                      "type": "string",
                      "optional": true
                    },
                    "organizational_unit_type": {
                      "type": "string",
                      "required": true
                    }
                  },
                  "block_types": {
                    "accounts": {
                      "block": {
                        "attributes": {
                          "account_email": {
                            "type": "string",
                            "optional": true
                          },
                          "account_id": {
                            "type": "string",
                            "optional": true
                          },
                          "account_name": {
                            "type": "string",
                            "required": true
                          },
                          "account_type": {
                            "type": "string",
                            "required": true
                          },
                          "phone": {
                            "type": "string",
                            "optional": true
                          }
                        }
                      },
                      "nesting_mode": "list"
