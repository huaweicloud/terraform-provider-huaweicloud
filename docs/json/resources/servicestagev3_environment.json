              "created_at": {
                "type": "string",
                "description": "The creation time of the environment, in RFC3339 format.",
                "computed": true
              },
              "creator": {
                "type": "string",
                "description": "The creator name of the environment.",
                "computed": true
              },
              "deploy_mode": {
                "type": "string",
                "description": "The deploy mode of the environment.",
                "optional": true,
                "computed": true,
