        "huaweicloud_geminidb_instance": {
          "block": {
            "attributes": {
              "actions": {
                "type": [
                  "list",
                  "string"
                ],
                "computed": true
              },
              "auto_renew": {
                "type": "string",
                "optional": true
              },
              "availability_zone": {
                "type": "string",
                "required": true
              },
              "charging_mode": {
                "type": "string",
                "optional": true
              },
              "configuration_id": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "created": {
                "type": "string",
                "computed": true
              },
              "db_user_name": {
                "type": "string",
                "computed": true
              },
              "dedicated_resource_id": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "delete_node_list": {
                "type": [
                  "set",
                  "string"
                ],
                "optional": true
              },
              "dual_active_info": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "destination_instance_id": "string",
                      "destination_region": "string",
                      "role": "string",
                      "status": "string"
                    }
                  ]
                ],
                "computed": true
              },
              "enterprise_project_id": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "groups": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "id": "string",
                      "nodes": [
                        "list",
                        [
