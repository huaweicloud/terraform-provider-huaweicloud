                    },
                    "storage_engine": {
                      "type": "string",
                      "required": true
                    },
                    "type": {
                      "type": "string",
                      "required": true
                    },
                    "version": {
                      "type": "string",
                      "required": true
                    },
                    "whole_version": {
                      "type": "string",
                      "computed": true
                    }
                  }
                },
                "nesting_mode": "list",
                "min_items": 1,
                "max_items": 1
              },
              "flavor": {
                "block": {
                  "attributes": {
                    "num": {
                      "type": "number",
                      "required": true
                    },
                    "size": {
                      "type": "number",
                      "required": true
                    },
                    "spec_code": {
                      "type": "string",
                      "required": true
                    },
                    "storage": {
                      "type": "string",
                      "required": true
                    }
                  }
                },
                "nesting_mode": "list",
                "min_items": 1,
                "max_items": 1
              }
            }
          }
        }
      }
    }
  }
}
