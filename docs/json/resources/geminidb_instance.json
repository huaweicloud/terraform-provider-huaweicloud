                            "availability_zone": "string",
                            "id": "string",
                            "name": "string",
                            "private_ip": "string",
                            "public_ip": "string",
                            "role": "string",
                            "spec_code": "string",
                            "status": "string",
                            "subnet_id": "string",
                            "support_reduce": "bool"
                          }
                        ]
                      ],
                      "status": "string",
                      "volume": [
                        "list",
                        [
                          "object",
                          {
                            "size": "string",
                            "used": "string"
                          }
                        ]
                      ]
                    }
                  ]
                ],
                "computed": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "lb_ip_address": {
                "type": "string",
                "computed": true
              },
              "lb_port": {
                "type": "string",
                "computed": true
              },
              "mode": {
                "type": "string",
                "required": true
              },
              "name": {
                "type": "string",
                "required": true
              },
              "password": {
                "type": "string",
                "required": true,
                "sensitive": true
              },
              "period": {
                "type": "number",
                "optional": true
              },
              "period_unit": {
                "type": "string",
                "optional": true
              },
              "port": {
                "type": "number",
                "optional": true,
                "computed": true
              },
              "product_type": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "security_group_id": {
                "type": "string",
                "required": true
              },
              "ssl_option": {
                "type": "string",
                "optional": true
              },
              "status": {
                "type": "string",
                "computed": true
              },
              "subnet_id": {
                "type": "string",
                "required": true
              },
              "tags": {
                "type": [
                  "map",
                  "string"
                ],
                "optional": true,
                "computed": true
              },
              "time_zone": {
                "type": "string",
                "computed": true
              },
              "updated": {
                "type": "string",
                "computed": true
              },
              "vpc_id": {
                "type": "string",
                "required": true
              }
            },
            "block_types": {
              "availability_zone_detail": {
                "block": {
                  "attributes": {
                    "primary_availability_zone": {
                      "type": "string",
                      "required": true
                    },
                    "secondary_availability_zone": {
                      "type": "string",
                      "required": true
                    }
                  }
                },
                "nesting_mode": "list",
                "max_items": 1
              },
              "backup_strategy": {
                "block": {
                  "attributes": {
                    "keep_days": {
                      "type": "number",
