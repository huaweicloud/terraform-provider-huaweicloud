{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_dns_zone": {
          "block": {
            "attributes": {
              "description": {
                "type": "string",
                "description": "The description of the zone.",
                "optional": true
              },
              "dnssec": {
                "type": "string",
                "description": "Specifies whether to enable DNSSEC for a public zone.",
                "optional": true,
                "computed": true
              },
              "dnssec_infos": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "created_at": "string",
                      "digest": "string",
                      "digest_algorithm": "string",
                      "digest_type": "number",
                      "ds_record": "string",
                      "flag": "number",
                      "key_tag": "number",
                      "ksk_public_key": "string",
                      "signature": "string",
                      "signature_type": "number",
                      "updated_at": "string"
                    }
                  ]
                ],
                "description": "Indicates the DNSSEC infos.",
                "computed": true
              },
              "email": {
                "type": "string",
                "description": "The email address of the administrator managing the zone.",
                "optional": true,
                "computed": true
              },
              "enterprise_project_id": {
                "type": "string",
                "description": "The enterprise project ID of the zone.",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "masters": {
                "type": [
                  "set",
                  "string"
                ],
                "description": "The list of the masters of the DNS server.",
                "computed": true
              },
              "name": {
                "type": "string",
                "description": "The name of the zone.",
                "required": true,
                "forcenew": true
              },
              "proxy_pattern": {
                "type": "string",
                "description": "The recursive resolution proxy mode for subdomains of the private zone.",
                "optional": true,
                "computed": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "status": {
                "type": "string",
                "description": "The status of the zone.",
                "optional": true,
                "computed": true
              },
              "tags": {
                "type": [
                  "map",
                  "string"
                ],
                "description": "The key/value pairs to associate with the zone.",
                "optional": true,
                "computed": true
              },
              "ttl": {
                "type": "number",
                "description": "The time to live (TTL) of the zone.",
                "optional": true,
                "default": 300
              },
              "zone_type": {
                "type": "string",
                "description": "The type of zone.",
                "optional": true,
                "forcenew": true,
                "default": "public"
              }
            },
            "block_types": {
              "router": {
                "block": {
                  "attributes": {
                    "router_id": {
                      "type": "string",
                      "description": "The ID of the associated VPC.",
                      "required": true
                    },
                    "router_region": {
                      "type": "string",
                      "description": "The region of the VPC.",
                      "optional": true,
                      "computed": true
                    }
                  }
                },
                "nesting_mode": "set"
              }
            }
          },
          "product": {
            "short": "DNS",
            "name": "Domain Name Service",
            "catalog": "Networking",
            "name_cn": "云解析服务",
            "catalog_cn": "网络"
          }
        }
      }
    }
  }
}
